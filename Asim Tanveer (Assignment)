function make_sandwich() {
    var items = [];
    for (var _i = 0; _i < arguments.length; _i++) {
        items[_i] = arguments[_i];
    }
    console.log("Sandwich Summary:");
    console.log("Bread +", items.join(" + "), "+ Bread");
    console.log();
}
make_sandwich("Ham", "Cheese");
make_sandwich("Turkey", "Lettuce", "Tomato");
make_sandwich("Peanut Butter", "Jelly", "Banana", "Honey");
function store_car_info(manufacturer, model) {
    var kwargs = [];
    for (var _i = 2; _i < arguments.length; _i++) {
        kwargs[_i - 2] = arguments[_i];
    }
    var carInfo = {
        manufacturer: manufacturer,
        model: model,
    };
    for (var _a = 0, kwargs_1 = kwargs; _a < kwargs_1.length; _a++) {
        var arg = kwargs_1[_a];
        var key = Object.keys(arg)[0];
        var value = arg[key];
        carInfo[key] = value;
    }
    return carInfo;
}
var car = store_car_info("toyota", "Model corolla 2023", { color: "Red" }, { optionalFeature: "Autopilot" });
console.log(car);
