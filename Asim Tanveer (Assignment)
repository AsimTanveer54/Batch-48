    console.log("You are in the infancy stage of life.");
}
else if (age < 13) {
    console.log("You are in the childhood stage of life.");
}
else if (age < 20) {
    console.log("You are in the teenage stage of life.");
}
else if (age < 65) {
    console.log("You are in the adulthood stage of life.");
}
else {
    console.log("You are in the senior stage of life.");
}
