var index = 10; // Intentionally set an out-of-bounds index
try {
    var value = numbers[index];
    console.log("Value at index", index, ":", value);
}
catch (error) {
    console.error("Error:", error.message);
}
finally {
    console.log("Correcting the error...");
}
// Variables for testing
var x = 10;
var y = 5;
var name = "John";
var age = 25;
var isStudent = true;
// Conditional tests
console.log("Conditional Tests:");
// Test 1
console.log("Test 1: x > y");
console.log("Prediction: true");
console.log("Result:", x > y);
// Test 2
console.log("Test 2: name === 'John'");
console.log("Prediction: true");
console.log("Result:", name === "John");
// Test 3
console.log("Test 3: age < 18");
console.log("Prediction: false");
console.log("Result:", age < 18);
// Test 4
console.log("Test 4: y >= 5 && isStudent");
console.log("Prediction: true");
console.log("Result:", y >= 5 && isStudent);
// Test 5
console.log("Test 5: x === 5 || age === 20");
console.log("Prediction: false");
console.log("Result:", x === 5 || age === 20);
