var alienColor = 'green';
if (alienColor === 'green') {
    console.log("You earned 5 points for shooting down the green alien!");
}
else if (alienColor === 'yellow') {
    console.log("You earned 10 points for shooting down the yellow alien!");
}
else if (alienColor === 'red') {
    console.log("You earned 15 points for shooting down the red alien!");
}
else {
    console.log("Invalid alien color.");
}
